                    <label style="color:white">Mail</label>
                    <div>
                        <InputText @bind-Value="oEmitirCotizacionAutoDTO.prendario.mail" class="form-control" placeholder="Mail"></InputText>
                    </div>
                </div>
            </div>
        </div>

        @*<button class="btn btn-success" type="submit">Emitir</button>*@

