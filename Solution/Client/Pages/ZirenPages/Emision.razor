                                }
                        </InputSelect>
                    </div>
                </div>

                <div class="col-md-5">
                    <label style="color:white">Localidad</label>
                    <div>
                        <InputText @bind-Value="oEmitirCotizacionAutoDTO.prendario.localidad" class="form-control" placeholder="Localidad"></InputText>
                    </div>
                </div>
                <div class="col-md-2">
                    <label style="color:white">CP.</label>
                    <div>
